<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="false">
        <exclude
            domain="root"
            path="." />
        <exclude
            domain="file"
            path="." />
        <exclude
            domain="database"
            path="." />
        <exclude
            domain="sharedpref"
            path="." />
        <exclude
            domain="external"
            path="." />
    </cloud-backup>
    <device-transfer>
        <exclude
            domain="root"
            path="." />
        <exclude
            domain="file"
            path="." />
        <exclude
            domain="database"
            path="." />
        <exclude
            domain="sharedpref"
            path="." />
        <exclude
            domain="external"
            path="." />
    </device-transfer>
</data-extraction-rules>